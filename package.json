  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "typecheck:web": "npm run typecheck --workspace=apps/web",
    "lint:web": "npm run lint --workspace=apps/web",
    "test:integrity": "prisma validate && prisma migrate status && npm run typecheck && npm run lint:check && npm run typecheck:web && npm run lint:web",
    "ngrok": "ngrok start --all",
    "db:seed": "ts-node scripts/seed-admin.ts"
  },
